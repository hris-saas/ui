{
    "name": "hris-saas/ui",
    "description": "HRis SaaS UI Package.",
    "keywords": [
        "hris-saas",
        "package",
        "ui"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Bertrand Kintanar",
            "email": "bertrand.kintanar@gmail.com",
            "role": "Creator & Designer"
        }
    ],
    "repositories": [
      {
        "type": "path",
        "url": "../core"
      }
    ],
    "require": {
        "hris-saas/core": "*",
        "facade/ignition": "^2.5",
        "intervention/image": "^2.5"
    },
    "support": {
        "issues": "https://gitlab.com/hris-saas/bugs/issues",
        "source": "https://gitlab.com/hris-saas/ui"
    },
    "autoload": {
        "psr-4": {
            "HRis\\UI\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HRis\\UI\\Providers\\UIServiceProvider",
                "HRis\\UI\\Providers\\RouteServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
